// Array Reversal
// The first line contains an integer, , denoting the size of the array. The next line contains  
// space-separated integers denoting the elements of the array.

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=n-1; i>=0; i--)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
