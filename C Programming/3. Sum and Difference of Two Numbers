#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int a,b;
    float c,d;
    scanf("%d %d",&a,&b);
	scanf("%f %f",&c,&d);
    int s1=a+b;
    int  s2=a-b;
    float f1=c+d;
    float f2=c-d;
    printf("%d %d\n",s1,s2);
    printf("%.1f %.1f",f1,f2);
    
    return 0;
}
